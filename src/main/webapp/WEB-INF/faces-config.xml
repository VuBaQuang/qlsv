<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">



    <navigation-rule>
        <from-view-id>/admin/class/info.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>update-student</from-outcome>
            <to-view-id>/admin/student/update-student.xhtml/update-student.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/class/info.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>add-student</from-outcome>
            <to-view-id>/admin/student/add-student.xhtml/add-student.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


</faces-config>
